  "scripts": {
    "seed-db": "node src/server/initDb",
    "server": "json-server --watch --port 4002 src/server/db.json",
    "start": "npm run seed-db && concurrently \"npm run server\" \"PORT=3005 craco start\"",
    "start:prod": "concurrently --kill-others \"npm run server\" \"npx serve build -l 3005\"",
    "start:all": "npm run seed-db && concurrently \"npm run server\" \"PORT=3005 craco start\" \"npm run cy:open\" \"npm run storybook\"",
    "build": "craco build",
    "test:jest": "craco test --collect-coverage",
    "test:cy": "cypress run --headless --browser chrome",
    "test:cy:open": "cypress open",
    "test": "npm run test:jest; npm run test:cy",


    "cy:run:all": "concurrently --kill-others \"npm run webpack:server\" \"npm run cy:run\"",
    "cy:open:all": "concurrently --kill-others \"npm run webpack:server\" \"npm run cy:open\"",

    "eject": "craco eject",
    "postinstall": "npm run seed-db",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "format": "npm run prettier:fix; npm run lint:fix",
    "typecheck": "tsc -p tsconfig.json",
    "documentation": "typedoc",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "cracoConfig": "craco.config.js",
  "main": "index.jsx",
  "license": "MIT"
}
